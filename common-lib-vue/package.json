{
  "name": "common-lib-vue",
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "main": "./dist/common-lib-vue.umd.cjs",
  "module": "./dist/common-lib-vue.js",
  "exports": {
    ".": {
      "import": "./dist/common-lib-vue.js",
      "require": "./dist/common-lib-vue.umd.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest  --coverage.enabled=true",
    "test:e2e": "start-server-and-test 'storybook dev -p 6006' 6006 'cypress open --e2e'",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@bcgov/bootstrap-v5-theme": "^1.0.1",
    "axios": "^1.7.2",
    "core-js": "^3.37.1",
    "date-fns": "^3.6.0",
    "maska": "^1.5.0",
    "mdn-polyfills": "^5.20.0",
    "pdfjs-dist": "^4.4.168",
    "sha1": "^1.1.1",
    "vue": "^3.4.21",
    "web-streams-polyfill": "^4.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-interactions": "^8.1.5",
    "@storybook/addon-links": "^8.1.5",
    "@storybook/addon-mdx-gfm": "^8.1.5",
    "@storybook/blocks": "^8.1.5",
    "@storybook/test": "^8.1.5",
    "@storybook/vue3": "^8.1.5",
    "@storybook/vue3-vite": "^8.1.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^2.0.3",
    "@vitest/web-worker": "^2.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.5",
    "bootstrap": "^4.3.1",
    "cypress": "^13.7.2",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.23.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.0",
    "start-server-and-test": "^2.0.3",
    "storybook": "^8.1.5",
    "storybook-addon-mock": "^5.0.0",
    "vite": "^5.2.12",
    "vitest": "^2.0.3"
  }
}
